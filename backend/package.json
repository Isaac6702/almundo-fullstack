    "seed-dev": "NODE_ENV=dev babel-node ./src/dump/index.js",
    "seed-test": "NODE_ENV=test babel-node ./src/dump/index.js",
    "seed-docker": "NODE_ENV=docker babel-node ./src/dump/index.js",
    "docker": "docker-compose up"
  },
